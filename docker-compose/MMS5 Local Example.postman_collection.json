		{
			"name": "query model",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "content-type",
						"value": "application/sparql-query",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "prefix foaf: <http://xmlns.com/foaf/0.1/>\nprefix : <https://mms.openmbee.org/demos/people/>\n\n        select ?name where {\n            ?s a :Person .\n            ?s foaf:name ?name .\n        }"
				},
				"url": {
					"raw": "{{host}}/orgs/testorg/repos/testrepo/branches/master/query",
					"host": [
						"{{host}}"
					],
					"path": [
